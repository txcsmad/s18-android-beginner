<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ahn.spring2018">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Scarne&apos;s Dice">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahn.spring2018.ProjectsActivity" />
        </activity>
        <activity
            android:name=".ProjectsActivity"
            android:label="Spring Projects">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScheduleActivity"
            android:label="Schedule">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahn.spring2018.ProjectsActivity" />
        </activity>
        <activity
            android:name=".EventsActivity"
            android:label="Events">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahn.spring2018.ScheduleActivity" />
        </activity>
        <activity
            android:name=".KittensActivity"
            android:label="Infinite Kittens">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahn.spring2018.ProjectsActivity" />
        </activity>
        <activity
            android:name=".FeedActivity"
            android:label="MADGram">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahn.spring2018.ProjectsActivity" />
        </activity>
        <activity android:name=".ComposeActivity"
            android:label="Compose Post">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ahn.spring2018.FeedActivity" />
        </activity>
    </application>

</manifest>